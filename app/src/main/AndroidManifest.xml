<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.karsol_ano">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/aarzuicon"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".PdfViewActivity" />
        <activity android:name=".PdfrenderActivity" />
        <activity android:name=".PdfWebview" />
        <activity
            android:name=".LoginModule.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".course.HomepageActivity" />
        <activity android:name=".course.BasicActivity" />
        <activity android:name=".course.CompanyActivity" />
        <activity android:name=".course.PartnershipActivity" />
        <activity android:name=".course.EnglishGujratiActivity" />
        <activity android:name=".MenuItems.Developed_Activity" />
        <activity android:name=".MenuItems.ContactUsActivity" />
        <activity android:name=".MenuItems.AboutUsActivity" />
        <activity android:name=".MenuItems.PriceActivity" />
        <activity android:name=".MenuItems.ChangePasswordActivity" />
        <activity android:name=".MenuItems.ForgotPasswordActivity"></activity>
    </application>

</manifest>